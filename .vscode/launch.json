    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYTHONIOENCODING": "utf-8"
            },
            "justMyCode": false
        },
        {
            "name": "Python: Module Baseline",
            "type": "python",
            "request": "launch",
            "module": "utils.baseline"
        },
        {
            "name": "Python: Module Utils",
            "type": "python",
            "request": "launch",
            "module": "utils.utils"
        },
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "2.tcp.ngrok.io",
                "port": 38433
            }
        },
        {
            "name": "dpso",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/dpso.py",
            "args": [
                "./data/data_single_instance_uniform_5.pt",
                "2",
                "random"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "active_search",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/active_search.py",
            "args": [
                "./data/data_single_instance_uniform_64.pt",
                "128"
            ],
            "console": "integratedTerminal",
        }
    ]
}